<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="canchita-pu" transaction-type="JTA">
	  <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
	  <jta-data-source>jdbc/canchita</jta-data-source>
    <class>com.canchita.models.Movie</class>
    <class>com.canchita.models.Genre</class>
    <class>com.canchita.models.User</class>
    <class>com.canchita.models.Schedule</class>
    <class>com.canchita.models.ScheduleItem</class>
    <class>com.canchita.models.Ticket</class>
    <class>com.canchita.models.AdminUser</class>
    <class>com.canchita.models.Comment</class>
    <shared-cache-mode>ENABLE_SELECTIVE</shared-cache-mode>
	</persistence-unit>
</persistence>
